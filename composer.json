{
    "name": "pouria/user-logs",
    "description": "Monitor your user and all activity on your application",
    "keywords": [
        "binafy",
        "milwad",
        "laravel",
        "framework",
        "monitoring",
        "tracking",
        "analysis",
        "laravel user monitoring package",
        "laravel user monitoring",
        "laravel package",
        "laravel-monitoring",
        "laravel-tracking",
        "laravel tracking",
        "laravel middleware"
    ],
    "homepage": "https://github.com/binafy/laravel-user-monitoring",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "pouria",
            "email": "pooria.azad53@gmail.com",
            "role": "developer",
            "homepage": "https://github.com/pouria-azad"
        },
        {
            "name": "milwad-dev",
            "email": "milwad.dev@gmail.com",
            "role": "author",
            "homepage": "https://github.com/milwad-dev"
        },
        {
            "name": "binafy",
            "role": "owner",
            "homepage": "https://github.com/binafy"
        }
    ],
    "autoload": {
        "files" : [
            "src/Helpers/helpers.php"
        ],
        "classmap": [
        "src/Helpers"
    ],
        "psr-4": {
            "Binafy\\LaravelUserMonitoring\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "pestphp/pest-plugin-laravel": "^1.4.0|^2.0.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html ./coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Binafy\\LaravelUserMonitoring\\Providers\\LaravelUserMonitoringServiceProvider"
            ]
        }
    }
}
